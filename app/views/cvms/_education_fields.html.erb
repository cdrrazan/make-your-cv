       <div class="nested-fields">
            <div class="left-col2">
                  <b>Level</b>
                  <%= f.input :level, label:false, :input_html => {:style=> 'width: 80px'}, placeholder: "Level" %>
                </div>

                <div class="center-col1">
                  <b>Institute *</b>
                  <%= f.input :institute, label:false, :input_html => {:style=> 'width: 270px'},  placeholder: "Enrolled Institute " %>
                </div>

                <div class="center-col2">
                  <b>Year *</b>
                  <%= f.input :year, label:false, :input_html => {:style=> 'width: 270px'}, placeholder: " Passed Year in AD" %>
                </div>

                <div class="right-col2">
                  <b>Percentage *</b>
                  <%= f.input :percentage, label:false, :input_html => {:style=> 'width: 280px'},placeholder: "Percentage " %>
                </div>
            <%= link_to_remove_association 'Remove', f, class: 'btn btn-danger btn-xs' %>
        </div>

