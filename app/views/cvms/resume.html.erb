<center><h1>Resume </h1></center>
 <h5><b>Name:</b> <%= "#{@cvm.fname} #{@cvm.mname} #{@cvm.lname}" %></h5>
            <h5><b>Adress:</b> <%= "#{@cvm.street}, #{@cvm.district}, #{@cvm.country}" %></h5>
            <h5><b>Phone:</b> <%= "Landline: #{@cvm.land} Mobile: #{@cvm.mobile}" %></h5>
            <h5><b>Email:</b> <%= @cvm.email %></h5>
            <h5><b>Website:</b> <%= "https://#{@cvm.website}" %></h5>
            <h5><b>Education:</b><br>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Level</th>
                        <th>Institute</th>
                        <th>Year</th>
                        <th>Percentage</th>
                    </tr>
                </thead>
                <tbody>
                    <%@cvm.educations.each do |education| %>
                    <tr>
                        <td><%= education.level%></td>
                        <td><%= education.institute %></td>
                        <td><%= education.year %></td>
                        <td><%= education.percentage %></td>
                    </tr>
                    <%end %>
                </tbody>
            </table>
            <br>
            </h5>
            <h5><b>Experience: </b>
            <table class="table table-striped">
                <% @cvm.experiences.each do |exp| %>
                <thead>
                    <tr>
                        <th>Company</th>
                        <th>Position</th>
                        <th>Detail</th>
                        <th>Start Date</th>
                        <th>End Date</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><%= exp.company%></td>
                        <td><%= exp.position %></td>
                        <td><%= exp.jobdetail %></td>
                        <td><%= exp.startdate %></td>
                        <td><%= exp.enddate %></td>
                    </tr>
                </tbody>
                <% exp.projects.each do |pro| %>
                <thead>
                    <tr>
                        <th>Project Name</th>
                        <th>Description</th>
                        <th>Start Date</th>
                        <th>End Date</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><%= pro.pname %></td>
                        <td><%= pro.description%></td>
                        <td><%= pro.startdate%></td>
                        <td><%= pro.enddate%></td>
                    </tr>
                </tbody>
                <hr><hr><hr><hr><hr><hr><hr>
                <%end %>
                <%end %>
            </table>
            </h5>
            <h5><b>Skill:</b> <%= @cvm.skill %></h5>
            <h5><b>Interest:</b> <%= @cvm.interest %></h5>
<br><br><br>
<p>
    <center>cdrrazan <%= Date.today.year %>. PDF generated via WickedPDF!</center>
</p>
